{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        /*"indent": [
            "error",
            "tab"
        ],*/
        "linebreak-style": [
            "error",
            "unix"
        ],
        /*"quotes": [
            "error",
            "double"
        ],*/
        "semi": [
            "error",
            "always"
        ],
        "no-undef": 1,
        "no-unused-vars": 1,
        "no-console": 1
    }
}